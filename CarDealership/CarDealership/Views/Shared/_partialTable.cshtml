@using CarDealership.Models.ViewModel
@model CarViewModel

<div class="container container-tabs position-relative">
    <div class="tab-content clearfix">
        <div class="tab-pane active" role="tabpanel">
            <table id="car-table" class="table table-bordered table-striped car-table">
                <thead class="table-primary">
                    <tr>
                        <th scope="col">Make</th>
                        <th scope="col">Year</th>
                        <th scope="col">Selected Options:</th>
                    </tr>
                </thead>
                @foreach (var carinfo in Model.data)
                {
                    <tr>
                        <td>@carinfo.Make</td>
                        <td>@carinfo.Year</td>
                        <td>@carinfo.PrintOptions()</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>